ext/na_op.c: mkop.rb
	$(RUBY) -I$(srcdir) $(srcdir)/mkop.rb

ext/na_op.o: ext/na_op.c ext/narray.h $(hdrdir)/ruby.h


ext/na_math.c: mkmath.rb
	$(RUBY) -I$(srcdir) $(srcdir)/mkmath.rb

ext/na_math.o: ext/na_math.c ext/narray.h $(hdrdir)/ruby.h


cleanall:  clean
	@$(RM) -r ext/na_op.c ext/na_math.c src pkg
